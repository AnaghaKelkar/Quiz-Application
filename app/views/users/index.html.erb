<div class="container">
  <div class="row">
    <center>
      <h3>Users</h3>
      <div class="column" style="left: 25%">
        <table class="table">
          <tbody>
            <% @users.each do |user| %>
              <tr>
                <%= render user %>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </center>
  </div>
</div>
